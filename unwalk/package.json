{
  "name": "movee",
  "private": true,
  "version": "1.0.1",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:app": "vite -c vite.dev.config.ts",
    "build": "tsc -b && vite build",
    "build:web": "tsc -b && vite build",
    "build:ios": "npm run build:ios:web && npx cap sync ios && npm run postcap:ios",
    "build:ios:web": "tsc -b && vite build -c vite.ios.config.ts",
    "open:ios": "npx cap open ios",
    "sync:ios": "npx cap sync ios",
    "postcap:ios": "node scripts/patch-ios-capacitor-config.mjs",
    "lint": "eslint .",
    "preview": "vite preview"
  },
  "dependencies": {
    "@capacitor/app": "^8.0.0",
    "@capacitor/cli": "^8.0.0",
    "@capacitor/core": "^8.0.0",
    "@capacitor/haptics": "^8.0.0",
    "@capacitor/ios": "^8.0.0",
    "@capacitor/preferences": "^8.0.0",
    "@capacitor/push-notifications": "^8.0.0",
    "@capacitor/splash-screen": "^8.0.0",
    "@capacitor/status-bar": "^8.0.0",
    "@capacitor/toast": "^8.0.0",
    "@supabase/supabase-js": "^2.87.1",
    "capacitor-movee-healthkit": "file:./plugins/movee-healthkit",
    "framer-motion": "^12.23.26",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-router-dom": "^7.10.1",
    "zustand": "^5.0.9"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
